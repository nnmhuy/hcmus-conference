emeColor } = props\n\n  return (\n    <div className={classes.root}>\n      <HideOnScroll {...props}>\n        <AppBar className={classes.appBar} style={{background: themeColor ? '#4053EB' : 'transparent'}}>\n          <Toolbar>\n            <div className={classes.leftSection}>\n              <Logo href='/'/>\n            </div>\n            <div className={classes.centerSection}>\n              <div className={classes.mainTitle}>\n                Hội nghị khoa học lần thứ XII - 2020\n              </div>\n            </div>\n            <div className={classes.rightSection}>\n              <NavBarItems/>\n            </div>\n          </Toolbar>\n        </AppBar>\n      </HideOnScroll>\n      <div className={classes.mainContainer}>\n        {children}\n      </div>\n    </div>\n  );\n}\n\nexport default DesktopLayout\n","/Users/minhhuy/WebProjects/hcmus-conference/src/components/Layout/MobileLayout.js",[],"/Users/minhhuy/WebProjects/hcmus-conference/src/components/Layout/constants.js",[],"/Users/minhhuy/WebProjects/hcmus-conference/src/components/Layout/components/NavBarItems.js",[],"/Users/minhhuy/WebProjects/hcmus-conference/src/constants/colors.js",[],"/Users/minhhuy/WebProjects/hcmus-conference/src/components/Layout/components/Logo.js",["189"],"/Users/minhhuy/WebProjects/hcmus-conference/src/components/Layout/components/SideBar.js",[],["190","191"],"/Users/minhhuy/WebProjects/hcmus-conference/src/pages/Schedule/index.js",[],"/Users/minhhuy/WebProjects/hcmus-conference/src/pages/Schedule/components/TimelineSection.js",[],"/Users/minhhuy/WebProjects/hcmus-conference/src/pages/Schedule/components/DateTabs.js",[],"/Users/minhhuy/WebProjects/hcmus-conference/src/constants/constants.js",[],"/Users/minhhuy/WebProjects/hcmus-conference/src/pages/Schedule/components/SessionTabs.js",[],"/Users/minhhuy/WebProjects/hcmus-conference/src/pages/Schedule/components/PresentationList.js",["192","193"],"/Users/minhhuy/WebProjects/hcmus-conference/src/pages/Schedule/components/P